{
  "name": "deploy-to-neocities",
  "version": "0.0.0",
  "description": "Github Action to deplpoy a folder to Neocities.org",
  "main": "index.js",
  "private": true,
  "scripts": {
    "test": "run-s test:*",
    "test-skip:deps": "dependency-check . --no-dev --no-peer",
